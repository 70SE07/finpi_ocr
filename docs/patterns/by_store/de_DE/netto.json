      "values": ["A", "B"],
      "meaning": {
        "A": {"rate": "7%", "description": "Reduced VAT"},
        "B": {"rate": "19%", "description": "Standard VAT"}
      },
      "inverted": false
    },
    
    "item_line": {
      "format": "name total tax"
    },
    
    "sku": {
      "present": false
    },
    
    "categories": {
      "present": false
    },
    
    "discounts": {
      "keywords": ["Rabatt"],
      "format": "separate_line_negative"
    },
    
    "pfand": {
      "present": true
    }
  },

  "validation": {
    "checksum_method": "SUM(items.total_price) == receipt_total",
    "all_receipts_passed": true,
    "discrepancy": 0.00
  },

  "notes": [
    "Standard tax (A=7%, B=19%) - NOT inverted",
    "Discount retailer",
