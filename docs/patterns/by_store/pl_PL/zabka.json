        "A": {"rate": "23%", "description": "Standard VAT"},
        "B": {"rate": "8%", "description": "Reduced VAT"},
        "C": {"rate": "5%", "description": "Super reduced VAT (food)"},
        "D": {"rate": "0%", "description": "Zero VAT"}
      },
      "position": "end_of_line",
      "inverted": false,
      "note": "Tax class shown as single letter suffix after total price"
    },

    "item_line": {
      "format": "name qty x unit_price = total tax_class",
      "examples": [
        "ESPRESSO 40ml GX-A        1szt. x4,99 4,99A",
        "KAWA Z MLEKIEM 200ml GX-A 1szt. x4,99 4,99A",
        "WKL TYT SIENNA-A          1szt. x19,99 19,99A"
      ],
      "quantity_position": "after_name",
      "quantity_format": "Nszt.",
      "multiline": false,
      "lines_per_item": 1
    },

    "sku": {
      "present": false,
      "note": "Internal codes like GX-A appear in product name, not as separate SKU"
    },

    "categories": {
      "present": false,
      "note": "No product categories on receipt"
    },

    "discounts": {
      "format": "unknown",
      "keywords": [],
      "note": "No discounts in analyzed receipts"
    },

    "date_format": {
      "format": "YYYY-MM-DD HH:MM",
      "example": "2025-11-18 08:43",
      "calendar": "gregorian",
      "note": "ISO format - unusual for Polish receipts!"
    },

    "totals": {
      "keywords": ["SUMA PLN"],
      "format": "SUMA PLN          value",
      "tax_keywords": ["SPRZEDAŻ OPODATKOWANA", "PTU"],
      "note": "PTU = Polish term for VAT"
    },

    "receipt_type": {
      "name": "PARAGON FISKALNY",
      "note": "Fiscal receipt (required by law)"
    },

    "franchise": {
